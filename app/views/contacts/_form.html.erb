<%= form_with(model:@contact, local:true) do |f| %>
  <div class="container ">
    <div class="row">
      <div class="col-6 p-3 m-3 border border-dark mx-auto">
        <h3 class="mt-4">Create Contact</h3>
        <div class="form-group mt-4">
          <%= f.label :fullname %>
          <%= f.text_field :fullname, class: 'form-control' %>
        </div>
        <div class="form-group mt-4">
          <%= f.label :phone %>
          <%= f.text_field :phone, class: 'form-control' %>
        </div>
        <div class="form-group mt-4">
          <%= f.label :email %>
          <%= f.text_field :email, class: 'form-control' %>
        </div>
        <div class="mt-4">
          <%= f.submit :class => 'btn btn-primary' %>
        </div>
      </div>
    </div>
  </div>
<%end%>